#/bin/sh
mv /usr/lib/libsqlite3.0.dylib /usr/lib/_libsqlite3.0.dylib
mv /usr/local/lib/libsqlite3.0.8.6.dylib /usr/local/lib/_libsqlite3.0.8.6.dylib

cd /usr/lib
rm -f libsqlite3.0.8.6.dylib
rm -f libsqlite3.dylib

cd /Developer/SDKs/MacOSX10.4u.sdk/usr/lib/
mv libsqlite3.0.dylib _libsqlite3.0.dylib
rm -f libsqlite3.0.8.6.dylib
rm -f libsqlite3.dylib

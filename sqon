#/bin/sh
mv /usr/lib/_libsqlite3.0.dylib /usr/lib/libsqlite3.0.dylib
mv /usr/local/lib/_libsqlite3.0.8.6.dylib /usr/local/lib/libsqlite3.0.8.6.dylib

cd /usr/lib
ln -s libsqlite3.0.dynlib libsqlite3.dylib 
ln -s libsqlite3.0.dynlib libsqlite3.0.8.6.dylib 

cd /Developer/SDKs/MacOSX10.4u.sdk/usr/lib/
mv _libsqlite3.0.dylib libsqlite3.0.dylib
ln -s libsqlite3.0.dynlib libsqlite3.dylib 
ln -s libsqlite3.0.dynlib libsqlite3.0.8.6.dylib 
